<?xml version='1.0' encoding='utf-8'?>
<widget id="org.eu.mawoka.sdreplacement" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SD-Client</name>
    <description>
        The client for the sd-replacement
    </description>
    <author email="mawoka@e.email" href="https://mawoka.eu.org">
        Mawoka
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <access origin="sentry.io" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AndroidXEnabled" value="true" />
</widget>
